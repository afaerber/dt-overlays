/dts-v1/;
/plugin/;

/ {
	compatible = "hardkernel,odroid-c2";

	#address-cells = <1>;
	#size-cells = <0>;

	fragment@0 {
		reg = <0>;

		target = <&ethmac>;

		__overlay__ {
			interrupts = <0x0 0x8 0x4>;
		};
	};

