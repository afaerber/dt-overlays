/dts-v1/;
/plugin/;

/ {
	compatible = "friendlyarm,nanopi-k2";

	#address-cells = <1>;
	#size-cells = <0>;

	fragment@0 {
		reg = <0>;

		target = <&uart_C>;

		__overlay__ {
			status = "okay";
			compatible = "amlogic,meson-gx-uart";
			clock-names = "xtal", "pclk", "baud";
		};
	};

	fragment@1 {
		reg = <1>;

		target-path = "/aliases";

		__overlay__ {
			serial1 = "/soc/cbus@c1100000/serial@8700";
		};
	};
};
