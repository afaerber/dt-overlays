/dts-v1/;
/plugin/;

/ {
	#address-cells = <1>;
	#size-cells = <0>;

	fragment@0 {
		reg = <0>;

		target = <&uart4>;

		__overlay__ {
			status = "okay";

			lora {
				compatible = "appconwireless,rf1276ts";
			};
		};
	};
};
