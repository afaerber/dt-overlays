/dts-v1/;
/plugin/;

/ {
	#address-cells = <1>;
	#size-cells = <0>;

	fragment@0 {
		reg = <0>;

		target = <&uart0>;

		__overlay__ {
			status = "okay";

			lora {
				compatible = "enocean,esp3";
				reset-gpios = <&gpio 5 0>;
			};
		};
	};
};
